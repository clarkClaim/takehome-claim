{
  "name": "claim",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "npx prisma generate && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/src/index.js",
    "pretest": "npm run build",
    "test": "node --expose-gc node_modules/jest/bin/jest.js --config=jest.config.js --silent=false --detectOpenHandles --logHeapUsage --forceExit",
    "predev": "npx prisma generate",
    "dev": "npx nodemon src/index.ts",
    "codegen": "ts-node src/graphql/codegen/codegen.ts && graphql-codegen --config src/graphql/codegen/codegen.yml",
    "lint": "eslint --ignore-pattern '*/**/*.{js,ts,tsx}' --fix --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "docker:up": "docker compose up -d",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:seed": "npx prisma db seed",
    "wait-for-postgres": "ts-node src/scripts/wait-for-postgres.ts",
    "quickstart": "npm run docker:up && npm run wait-for-postgres && npm run prisma:reset && npm run prisma:migrate && npm run prisma:seed && (npm run prisma:studio & npm run dev)"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-resolvers": "2.7.12",
    "@types/jest": "^29.5.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.11.18",
    "@types/node-schedule": "^2.1.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "prisma": "^5.19.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^4.3.2",
    "@apollo/server": "^4.3.0",
    "@pothos/core": "3.41.0",
    "@pothos/plugin-prisma": "3.65.0",
    "@pothos/plugin-scope-auth": "^3.19.2",
    "@prisma/client": "^5.19.1",
    "@types/cors": "^2.8.13",
    "@types/lodash.shuffle": "^4.2.9",
    "body-parser": "^1.20.2",
    "core-js": "^3.36.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.1"
  }
}
